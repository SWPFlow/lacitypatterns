<div class="page" id="page">
	{{> organisms-header }}
	{{# hero}}
		{{> molecules-block-hero-secondary }}
	{{/ hero}}
	<div role="main">
    {{> molecules-breadcrumbs }}
		<div class="l-two-col">
			<div class="l-main">
				<article class="article">
					<header class="article-header">
						<h1>Article Headline Lorem ipsum dolor sit aweofij</h1>
						{{> molecules-byline }}
					</header>
					{{> organisms-article-body }}
				</article><!--end .article-->
				{{> organisms-comment-thread }}
			</div><!--end l-main-->

			<div class="l-sidebar">
				{{> organisms-related-posts }}
				{{> organisms-recent-tweets }}
				{{> organisms-poll }}
			</div><!--end l-sidebar-->
		</div><!--end l-two-col-->	
	</div><!--End role=main-->
	{{> organisms-slab-posts }}
	{{> organisms-footer }}
	{{> molecules-social-share }}
</div>